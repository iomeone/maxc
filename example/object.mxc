object Vector {
    x: int, y: int
}

fn `+`(a: Vector, b: Vector): Vector {
    let vec = new Vector {};

    vec.x = a.x + b.x;
    vec.y = a.y + b.y;

    return vec;
}

fn put(self: Vector) {
    println(self.x, " ", self.y);
}

let a = new Vector{};
let b = new Vector{};

a.x = 103; a.y = 110;
b.x = 201; b.y = 319;

let res = a + b;

res.put();
