fn fizzbuzz(n: int) {
    let i = 1;
    while i <= n {
        if i % 15 == 0 {
            print("fizzbuzz");
        }
        else if i % 5 == 0 {
            print("buzz");
        }
        else if i % 3 == 0 {
            print("fizz");
        }
        else {
            print(i);
        }

        ++i;
    }
    println("");
}

400.fizzbuzz();
