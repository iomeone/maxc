fn fizzbuzz(n: int) -> int {
    let i:int = 1;
    for(i = 1; i <= n; ++i) {
        if(i % 3 == 0 && i % 5 == 0)
            print("fizzbuzz");
        else if(i % 3 == 0)
            print("fizz");
        else if(i % 5 == 0)
            print("buzz");
        else
            print(i);
        print(" ");
    }
    println("");
}

fizzbuzz(400);
